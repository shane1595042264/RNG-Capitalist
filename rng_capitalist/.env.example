# Environment Variables for RNG Capitalist
# Copy this file to .env and fill in your actual values

# Google Gemini AI API Key
# Get your free API key from: https://aistudio.google.com/app/apikey
GOOGLE_GEMINI_API_KEY=your_api_key_here

# Firebase Configuration
# Get these values from your Firebase project settings
# Visit: https://console.firebase.google.com/
FIREBASE_API_KEY_WEB=your_firebase_web_api_key_here
FIREBASE_API_KEY_ANDROID=your_firebase_android_api_key_here
FIREBASE_API_KEY_IOS=your_firebase_ios_api_key_here
FIREBASE_API_KEY_MACOS=your_firebase_macos_api_key_here
FIREBASE_API_KEY_WINDOWS=your_firebase_windows_api_key_here
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
FIREBASE_APP_ID_WEB=your_firebase_web_app_id
FIREBASE_APP_ID_ANDROID=your_firebase_android_app_id
FIREBASE_APP_ID_IOS=your_firebase_ios_app_id
FIREBASE_APP_ID_MACOS=your_firebase_macos_app_id
FIREBASE_APP_ID_WINDOWS=your_firebase_windows_app_id
FIREBASE_MEASUREMENT_ID=your_measurement_id
FIREBASE_STORAGE_BUCKET=your_storage_bucket

# Other configuration
APP_ENV=production
DEBUG_MODE=false
